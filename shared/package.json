{
  "name": "@maas-ui/maas-ui-shared",
  "version": "3.2.0",
  "license": "AGPL-3.0",
  "private": true,
  "main": "dist/index.js",
  "files": [
    "src/**/*.js",
    "dist/**/*.js",
    "!/dist/**/*.test.js"
  ],
  "scripts": {
    "build": "yarn build-declaration && babel src --out-dir dist --extensions '.js,.jsx,.ts,.tsx'",
    "build-declaration": "tsc",
    "clean": "rm -rf node_modules dist",
    "lint": "yarn lint-package-json && yarn lint-js",
    "lint-js": "eslint src",
    "lint-package-json": "npmPkgJsonLint .",
    "prepublishOnly": "yarn build",
    "test": "jest --testPathIgnorePatterns=dist",
    "watch": "yarn build --watch",
    "watch-declaration": "yarn build-declaration --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.17.6",
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.3",
    "@testing-library/user-event": "13.5.0",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "babel-plugin-inline-dotenv": "1.6.0",
    "eslint": "7.32.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.10.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-no-only-tests": "2.6.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.2",
    "eslint-plugin-react-hooks": "4.3.0",
    "jest": "27.5.1",
    "mockdate": "3.0.5",
    "npm-package-json-lint": "5.4.2"
  },
  "dependencies": {
    "@types/google.analytics": "0.0.42",
    "@types/history": "4.7.11",
    "@types/jest": "27.4.1",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "typescript": "4.5.5"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  }
}
